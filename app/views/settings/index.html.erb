<h1>Listing settings</h1>

<table>
  <tr>
    <th>Canvas url</th>
    <th>Deauthorize callback url</th>
    <th>Type</th>
    <th>Status</th>
    <th>Name</th>
    <th>Facebook</th>
    <th>Facebook secret</th>
    <th>Facebook canvas page</th>
    <th>Tiny url</th>
    <th>Event</th>
    <th>Event message</th>
    <th>Event rsvp</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @settings.each do |setting| %>
  <tr>
    <td><%= setting.canvas_url %></td>
    <td><%= setting.deauthorize_callback_url %></td>
    <td><%= setting.type %></td>
    <td><%= setting.status %></td>
    <td><%= setting.name %></td>
    <td><%= setting.facebook_id %></td>
    <td><%= setting.facebook_secret %></td>
    <td><%= setting.facebook_canvas_page %></td>
    <td><%= setting.tiny_url %></td>
    <td><%= setting.event_id %></td>
    <td><%= setting.event_message %></td>
    <td><%= setting.event_rsvp %></td>
    <td><%= link_to 'Show', setting %></td>
    <td><%= link_to 'Edit', edit_setting_path(setting) %></td>
    <td><%= link_to 'Destroy', setting, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Setting', new_setting_path %>
